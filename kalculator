#include <iostream>
using namespace std;

int main() {
    char op;
    double x, y;

    cout << "(+, -, *, /): ";
    cin >> op;

    cout << "x,y: ";
    cin >> x >> y;

    switch(op) {
        case '+':
            cout << x << " + " << y << " = " << x + y << endl;
            break;
       case '-':
            cout << x << " - " << y << " = " << x + y << endl;
            break;
        case '*':
            cout << x << " * " << y << " = " << x + y << endl;
            break;
       case '/':
            cout << x << " / " << y << " = " << x + y << endl;
            break;

    }
    return 0;
}
